<div class="bg faq">
  <div class="container">
    <div class="row">
      <div class="col-lg-6 col-lg-offset-6 col-md-6 col-md-offset-6 col-sm-8 col-sm-offset-4 col-xs-12" style="margin-top: 15%; color: white;">
        <h1>FAQ</h1>
        <% @faqs.each do |faq| %>
          <h4><%= faq.question %></h4>
          <p><%= faq.answer %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>
